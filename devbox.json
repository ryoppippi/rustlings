{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.12.0/.schema/devbox.schema.json",
  "packages": [
    "cargo@latest",
    "rust-analyzer@latest",
    "rustlings@latest"
  ],
  "shell": {
    "init_hook": [
      "echo 'cargo'",
      "which cargo",
      "cargo --version"
    ],
    "scripts": {
      "edit": "$EDITOR",
      "dev": "rustlings"
    }
  }
}
